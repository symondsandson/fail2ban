# generated by chef

[<%= @name %>]

enabled = <%= @enabled %>
<% %w{ port filter logpath findtime bantime maxretry protocol banaction backend ignorecommand }.each do |key| %>
<% next unless value = self.instance_variable_get("@#{key}".to_sym) %>
<%= key %> = <%= value %>
<% end %>
<% if @actions %>
<% if @actions =~ /action_/ %>
action = %(<%= @actions %>)s
<% else %>
action = <%= @actions %>
<% end %>
<% end %>
